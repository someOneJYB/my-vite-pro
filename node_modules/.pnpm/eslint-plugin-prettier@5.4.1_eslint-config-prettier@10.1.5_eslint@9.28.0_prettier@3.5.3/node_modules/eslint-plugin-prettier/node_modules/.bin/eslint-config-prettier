#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint-config-prettier@10.1.5_eslint@9.28.0/node_modules/eslint-config-prettier/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../eslint-config-prettier@10.1.5_eslint@9.28.0/node_modules/eslint-config-prettier/bin/cli.js" "$@"
fi
